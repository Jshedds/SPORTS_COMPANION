<div class="container chatroom" data-controller="chatroom-subscription" data-chatroom-subscription-chatroom-name-value="<%= @chatroom.name %>">


    <div class="chatroom-title">
      <%= @sport.name.upcase %> CHATROOM
    </div>
  <div class="chatroom-btns-container">

    <div id="chatroom-back-btn">
      <%= link_to render_icon("back"), sport_path(@sport) %>
    </div>
  </div>



  <div class="messages" data-chatroom-subscription-target="messages" data-aos="fade-up">
    <% @chatroom.messages.each do |message| %>
      <div>
        <%= render "messages/message", message: message %>
      </div>
    <% end %>
  </div>

  <div class="message-board w-100" id="chatroom-div">
    <div data-chatroom-subscription-target="submit" class="w-100">

      <%= simple_form_for [@sport, @chatroom, @message],
        html: {class: "d-flex w-100 justify-content-between",
          data: {
            action: "turbo:submit-end->chatroom-subscription#resetForm"
          }
        } do |f|
      %>
        <div class="message-new">
          <%= f.input :content,
            label: false,
            placeholder: "MESSAGE CHATROOM",
            wrapper_html: {class: "flex-grow-1"}
          %>
        </div>
